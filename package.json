{
  "name": "AZIT_Front",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=web",
    "dev:mobile": "pnpm --filter mobile start",
    "build:web": "turbo run build --filter=web",
    "build:mobile": "pnpm --filter mobile build",
    "lint": "turbo run lint",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,yaml,yml}\"",
    "prettier:write": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,yaml,yml}\"",
    "typecheck": "turbo run typecheck"
  },
  "devDependencies": {
    "@turbo/gen": "^2.7.2",
    "eslint": "catalog:",
    "prettier": "^3.7.4",
    "turbo": "^2.7.2",
    "typescript": "catalog:"
  },
  "packageManager": "pnpm@10.26.0",
  "engines": {
    "node": ">=18"
  }
}
